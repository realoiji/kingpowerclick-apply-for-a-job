{
  "extends": ["@imaginary-cloud/react"],
  // more configuration options here!
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error", "debug"] }]
  }
}
